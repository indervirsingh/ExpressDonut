{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "node10",
    "strict": true,
    "outDir": "./built",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "target": "ES2022",
    "allowJs": true,
    "noEmit": false,
    "moduleDetection": "force",
    "resolveJsonModule": true
  },
  "include": ["src/**/*"]
}